{
  "name": "codedocs/codedocs",
  "description": "Let your project documentation automatically update when your code changes!",
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Blos",
      "email": "christian.blos@gmx.de"
    }
  ],
  "require": {
    "php": "^8.1",
    "nikic/php-parser": "^v5.7.0",
    "doctrine/annotations": "^1.2",
    "christianblos/markdown2html": "^0.4",
    "erusev/parsedown-extra": "0.9.0 as 0.8.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "bin": [
    "codedocs"
  ],
  "autoload": {
    "psr-4": {
      "CodeDocs\\": [
        "src/",
        "annotations/"
      ],
      "CodeDocs\\Func\\": "functions"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.x-dev"
    }
  }
}
